    AnyVar py = 0.003 - 0.004 ;     //IS : Superieur = +  
    AnyVar pz = 0.001 - 0.0046;     //ML : Lateral = +
    #endif
    
    #if HumExternalRotation == -21
    AnyVar px = -.002 + 0.0005;     //AP : Anterieur = +   
    AnyVar py = 0.003 - 0.004 ;     //IS : Superieur = +  
    AnyVar pz = 0.001 - 0.0046;     //ML : Lateral = +
    #endif
    
    
    
    // Pour que l'angle entre les implants soit similaire  Lauranne    
    #if HumExternalRotation == -16
    
    // Values for case 5 calibration 
    //    AnyVar px = -.002;     //AP : Anterieur = +   
    //    AnyVar py = 0.003 -0.003;     //IS : Superieur = +  
    //    AnyVar pz = 0.001 - 0.0048;     //ML : Lateral = +
    
    // Values for case 3 calibration 
    AnyVar px = -.002;     //AP : Anterieur = +   
    AnyVar py = 0.003 -0.003;     //IS : Superieur = +  
    AnyVar pz = 0.001 - 0.0049;     //ML : Lateral = +
    
    
    
    // test 
    //    AnyVar px = -.002 -0.001;     //AP : Anterieur = +   
    //    AnyVar py = 0.003 - 0.004 + 0.003;     //IS : Superieur = +  
    //    AnyVar pz = 0.001 - 0.0044;     //ML : Lateral = +    
    #endif
    
    // Implants parfaitement en faces
    #if HumExternalRotation == -10
