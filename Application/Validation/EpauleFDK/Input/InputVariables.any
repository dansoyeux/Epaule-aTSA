  #define ResultFile "Anybody-" + CSA_Case 
  #endif
  
  #define LoadFile "04-01-" + CSA_Case + "-GlenoidAxisTilt-MR_Polynomial-Elevation-no-recentrage"
  #define LoadFolder "SaveData/Elevation_no_recentrage"  
  
